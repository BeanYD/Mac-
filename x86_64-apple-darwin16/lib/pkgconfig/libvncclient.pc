prefix=@@CONTRIB_PREFIX@@
exec_prefix=${prefix}
libdir=@@CONTRIB_PREFIX@@/lib
includedir=@@CONTRIB_PREFIX@@/include

Name: LibVNCClient
Description: A library for easy implementation of a VNC client.
Version: 0.9.10
